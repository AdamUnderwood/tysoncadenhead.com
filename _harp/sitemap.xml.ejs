<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.google.com/schemas/sitemap/0.84" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.google.com/schemas/sitemap/0.84 http://www.google.com/schemas/sitemap/0.84/sitemap.xsd">

        <url>
            <loc><%- url %></loc>
            <lastmod><%- new Date().getFullYear() %>-<%- new Date().getMonth() %>-<%- new Date().getDate() %> 12:12</lastmod>
            <changefreq>daily</changefreq>
            <priority>1.0</priority>
        </url>

        <url>
            <loc><%- url %>/about</loc>
            <lastmod><%- new Date().getFullYear() %>-<%- new Date().getMonth() %>-<%- new Date().getDate() %> 12:12</lastmod>
            <changefreq>daily</changefreq>
            <priority>1.0</priority>
        </url>

        <url>
            <loc><%- url %>/presentations</loc>
            <lastmod><%- new Date().getFullYear() %>-<%- new Date().getMonth() %>-<%- new Date().getDate() %> 12:12</lastmod>
            <changefreq>daily</changefreq>
            <priority>1.0</priority>
        </url>

    <% for(post in public.blog._data) { %>
        <% if (public.blog._data[post].published) { %>
            <url>
                <loc><%- url %>/blog/<%- post %></loc>
                <lastmod><%- public.blog._data[post].date.split('-')[2] %>-<%- public.blog._data[post].date.split('-')[0] %>-<%- public.blog._data[post].date.split('-')[1] %> 12:12</lastmod>
                <changefreq>daily</changefreq>
                <priority>1.0</priority>
            </url>
        <% } %>
    <% } %>
</urlset>